    <style name="BaseNoActionBarTheme" parent="Theme.AppCompat.Light.NoActionBar" />

    <style name="AppTheme" parent="BaseNoActionBarTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/titelbar_color</item>
        <item name="colorPrimaryDark">@color/titelbar_color</item>
        <item name="colorAccent">@color/titelbar_color</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="iconColor">@color/buttonBlueTitleBarColor</item>
        <item name="iconRefrenceColor">@color/buttonRefrenceBlueTitleBarColor</item>
        <item name="titleColor">@color/white</item>
    </style>
    <style name="OverlayThemeBlue" parent="AppTheme">
        <item name="colorPrimary">@color/titelbar_color</item>
        <item name="colorPrimaryDark">@color/titelbar_color</item>
        <item name="colorAccent">@color/titelbar_color</item>
        <item name="iconColor">@color/buttonBlueTitleBarColor</item>
        <item name="iconRefrenceColor">@color/buttonRefrenceBlueTitleBarColor</item>
        <item name="titleColor">@color/white</item>
        <item name="otpViewStyle">@style/OtpWidget.OtpView</item>
    </style>
    <style name="SpinnerTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/bg_spinner</item>
    </style>
    <style name="OverlayThemeRed" parent="AppTheme">
        <item name="colorPrimary">@color/redThemeColor</item>
        <item name="colorPrimaryDark">@color/redThemeColor</item>
        <item name="colorAccent">@color/redThemeColor</item>
        <item name="iconColor">@color/buttonRedTitleBarColor</item>
        <item name="iconRefrenceColor">@color/buttonRefrenceRedTitleBarColor</item>
        <item name="titleColor">@color/black</item>
        <item name="otpViewStyle">@style/OtpWidget.OtpView</item>
    </style>

